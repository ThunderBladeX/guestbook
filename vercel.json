{
  "version": 2,
  "builds": [
    {
      "src": "guestbook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/favicon\\.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/favicon\\.png",
      "dest": "/favicon.png"
    },
    {
      "src": "/(.*)",
      "dest": "guestbook.py"
    }
  ]
}
