{
  "version": 2,
  "builds": [
    {
      "src": "guestbook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "guestbook.py"
    }
  ],
  "env": {
      "FLASK_APP": "guestbook.py",
      "FLASK_ENV": "production",
      "ADMIN_KEY": "@admin-key-secret"
  }
}
